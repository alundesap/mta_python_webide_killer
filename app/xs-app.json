{
	"welcomeFile": "index.html",
	"authenticationMethod": "route",
	"routes": [
	{
		"source": "(python\/)(.*)",
		"destination": "python_be", 
		"csrfProtection": true,
		"authenticationType": "none"
	},
	{
		"source": "(node\/)(.*)",
		"destination": "node_be", 
		"csrfProtection": true,
		"authenticationType": "none"
	},
	{
		"localDir": "resources",
		"source": "^/(.*)", 
		"authenticationType": "none"
	}
	]
}
